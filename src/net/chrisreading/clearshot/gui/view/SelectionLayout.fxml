            <Tooltip text="Upload" />
         </tooltip></Button>
      <Button id="cancel" fx:id="btnCancel" mnemonicParsing="false" onAction="#onCancel" prefHeight="30.0" prefWidth="40.0" GridPane.columnIndex="3">
         <tooltip>
            <Tooltip text="Cancel" />
         </tooltip></Button>
   </children>
</GridPane>
